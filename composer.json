{
    "name": "amarneche/cpanel-api",
    "description": "Simple Collection of  api calls to manage Cpanel account using URLS",
    "autoload": {
        "psr-4": {
            "Amarneche\\CpanelApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Amar Neche",
            "email": "contact@amarneche.me"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "7.0"
    },
    "extra": {
        "laravel":{
            "providers":[
                "Amarneche\\CpanelApi\\CpanelServiceProvider"
            ],
            "aliases":[
                "Amarneche\\CpanelApi\\Cpanel"
            ]
        }
    }
}
